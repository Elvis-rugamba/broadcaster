{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "eol-last": "off",
        "linebreak-style": 0,
        "func-names": "off",
        "no-process-exit": "off",
        "no-process-env": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "consistent-return": "off",
        "no-useless-escape": "off",
        "max-len": ["error", 120, 2, {
            "ignoreUrls": true,
            "ignoreComments": false,
            "ignoreRegExpLiterals": true,
            "ignoreStrings": true,
            "ignoreTemplateLiterals": true
          }]
    }
}